FoamFile
{
    version     3.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      forceCoeffsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//change readfields_custom with the wanted fields to read, then write the correct fields in the forceCoeffs and then, after uncommenting the forceCoeffsDict in controlDict, run solver -postProcess
readFields_custom
{
    type                readFields;
    libs                ("libfieldFunctionObjects.so");  // Changed library
    functionObjectLibs  ("libfieldFunctionObjects.so");
    fields              (URomGalerkin28Modes pRomGalerkin28Modes
                         URomGalerkinPressure28Modes pRomGalerkinPressure28Modes
                         URomPPE28Modes pRomPPE28Modes
                        URomPPE45Modes pRomPPE45Modes
                        URomPPE60Modes pRomPPE60Modes 
                            );
    
    // Ensure it runs first
    timeStart 100;
    timeEnd   200;
    executeControl writeTime;
    //executeInterval 1;
    //writeControl    onEnd;
    
}

forceCoeffsFOM
{
    // Mandatory entries
    type            forceCoeffs;
    libs            ("libforces.so");
    writeAtStart    no;
    timeStart 100;
    timeEnd   200;
    writeControl    writeTime;

    patches         (cylinder1);

    // Optional entries

    // Field names
    U      U;
    p      p;
    rho     rhoInf;
    rhoInf  1.0;

    // Centre of rotation for moment calculations
    CofR            (0 0 0);

    // Lift direction
    liftDir         (0 1 0);

    // Drag direction
    dragDir         (1 0 0);

    // Pitch axis
    pitchAxis       (0 0 1);

    // Freestream velocity magnitude [m/s]
    magUInf         1;

    // Reference length [m]
    lRef            1;

    // Reference area [m2]
    Aref            1;
}


forceCoeffsGalerkin
{
    // Mandatory entries
    type            forceCoeffs;
    libs            ("libforces.so");
    writeControl    writeTime;
    timeStart 100;
    timeEnd   200;

    patches         (cylinder1);

    // Optional entries

    // Field names
    U      URomGalerkin28Modes;
    p      pRomGalerkin28Modes;
    rho     rhoInf;
    rhoInf  1.0;

    // Centre of rotation for moment calculations
    CofR            (0 0 0);

    // Lift direction
    liftDir         (0 1 0);

    // Drag direction
    dragDir         (1 0 0);

    // Pitch axis
    pitchAxis       (0 0 1);

    // Freestream velocity magnitude [m/s]
    magUInf         1;

    // Reference length [m]
    lRef            1;

    // Reference area [m2]
    Aref            1;
}


